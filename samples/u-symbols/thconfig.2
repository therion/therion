#! section of your layout, you redefine point symbol like this
#!CODE
def p_u(expr pos, theta, sc, al, subtype) = 
  if subtype = "bat": % case of bat symbol
    T := identity shifted pos;
    thfill (bat_path scaled 2.0) withcolor black;
  else: % use default symbol for other cases
    p_u_SKBB(pos, theta, sc, al, subtype);
  fi;
enddef;
#!ENDCODE

#! same way the line symbol
#!CODE
def l_u(expr P, subtype) =
  if subtype = "bat": % case of bat line
    T:=identity;
    cas := 0;
    dlzka := arclength P;
    mojkrok:=adjust_step(dlzka, 1.0u);
    pickup PenD;
    forever:
      t := arctime cas of P;
      thfill bat_path scaled 0.5 shifted (point t of P) withcolor black;
      cas := cas + mojkrok;
      exitif cas > dlzka + (mojkrok / 3); % for rounding errors
    endfor;
  else: % use default u line for other cases
    p_l_SKBB(P, subtype);
  fi;
enddef;
#!ENDCODE

#! and area symbol (pattern in this case)
#!CODE
% bat pattern
beginpattern(pattern_bat);
    fill bat_path withcolor black;
endpattern;

% bat area symbol
def a_u (expr Path, subtype) =
  if subtype = "bat": % bat area
    T:=identity;
    thclean Path;
    thfill Path withpattern pattern_bat;
  else: % use default u area for other cases
    a_u_SKBB(Path, subtype);
  fi;
enddef;
#!ENDCODE

endlayout

